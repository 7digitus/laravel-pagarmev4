{
    "name": "7digitus/laravel-pagarme",
    "description": "Integração da API PHP PagarMe com o Laravel",
    "type": "library",
    "require": {
        "php": "^7.0.0",
        "pagarme/pagarme-php": "^3.9.0"
    },
    "authors": [
        {
            "name": "7digitus"
        }
    ],
    "autoload": {
        "psr-4": {
            "RogerArruda\\LaravelPagarMe\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "7digitus\\LaravelPagarMeV4\\Providers\\PagarMeServiceProvider"
            ],
            "aliases": {
                "PagarMe": "7digitus\\LaravelPagarMeV4\\Facades\\PagarMeFacade"
            }
        }
    }
}
